{
  "markdown.marp.themes": [
    "./theme.css"
  ],
  "name": "marp-template",
  "version": "1.0.0",
  "description": "Marp custom theme template for Go Saito",
  "scripts": {
    "serve": "marp -s ./ --theme ./theme.css",
    "build:html": "marp ./sample.md --theme ./theme.css -o ./dist/sample.html",
    "build:pdf": "marp ./sample.md --pdf --pdf-notes --pdf-outlines --allow-local-files --theme ./theme.css -o ./dist/sample.pdf",
    "build:pptx": "marp ./sample.md --pptx --pptx-editable --allow-local-files --theme ./theme.css -o ./dist/sample.pptx"
  },
  "author": "Hiro",
  "license": "MIT",
  "devDependencies": {
    "@marp-team/marp-cli": "^4.2.2"
  }
}
